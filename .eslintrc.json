{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "parser": "babel-eslint",
  "rules": {
      
    "camelcase": [
        "warn",
        {
          "properties": "never"
        }
      ],
    "no-console": 0,
    "no-empty": 0,
    "no-irregular-whitespace": 0,
    "react/prop-types": "off"
  },
  "settings": {
    "react": {
      "version": "17.0.2"
    }
  }
}
